			<patient-id column="PatID"/>
			<visit-id column="VisitID"/>
			<start column="VisitID" format="d.M.u[ H[:m[:s]]]" na=""/>
		</idat>
		<mdat/>
		<ignore column="*" xsi:type="string"/>	
	</visit-table>
